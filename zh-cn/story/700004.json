                {
                  "Name": "ReignsChangeBackground",
                  "Params": {
                    "Background": "/Game/Aki/UI/UIResources/Common/Image/BgCgBig/T_ReignsBg6.T_ReignsBg6",
                    "ChangeMode": 1
                  },
                  "ActionGuid": "1d8952559a444583a2134a92c5b0dc06",
                  "ActionId": 5000410
                }
              ]
            }
          ]
        },
        {
          "Id": 9,
          "Speaker": "Speaker_83",
          "PlotLineKey": "时间终于来到了试炼日。<br>你带着“新铸”的剑走到竞技场中央，<br>青铜号角在十二根石柱的顶端齐鸣。<br>所有人都想见证人类的力量能否撼动亚狮诺索。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1579,
              "PlotLineKey": "环视四周。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4081
                  },
                  "ActionGuid": "0ccec8e29a344780ab866364bf65afb3",
                  "ActionId": 5000360
                }
              ]
            },
            {
              "TextId": 1580,
              "PlotLineKey": "深吸一口气。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4082
                  },
                  "ActionGuid": "00eaf79792fb46ada6868123c3c10242",
                  "ActionId": 5000361
                }
              ]
            }
          ]
        },
        {
          "Id": 10,
          "Speaker": "Speaker_83",
          "PlotLineKey": "忽地，看台上的呼喊声被某种邃古的威势截断，<br>飘动的贵族旌旗霎地垂落。<br>巨大的阴影将你周身的阳光完全遮蔽——亚狮诺索现身了。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1581,
              "PlotLineKey": "注意到亚狮诺索高大如山的身形。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4091
                  },
                  "ActionGuid": "eda67f2f0bbd466f84fabe706c4af87f",
                  "ActionId": 5000362
                }
              ]
            },
            {
              "TextId": 1582,
              "PlotLineKey": "有些耳鸣。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4092
                  },
                  "ActionGuid": "accd8912444140638b171953b2ad57e1",
                  "ActionId": 5000363
                }
              ]
            }
          ]
        },
        {
          "Id": 11,
          "Speaker": "Speaker_83",
          "PlotLineKey": "“它醒了！”某个贵族尖声叫道。<br>此时亚狮诺索正沉默地伫立在你对面，<br>它缓缓睁开双瞳，冷冷地审视着你。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1583,
              "PlotLineKey": "提高警惕。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4101
                  },
                  "ActionGuid": "2eae4683c7584d5a84e5b11a59d28159",
                  "ActionId": 5000364
                }
              ]
            },
            {
              "TextId": 1584,
              "PlotLineKey": "按住剑柄。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4102
                  },
                  "ActionGuid": "9f1616b132fd43b68075b42e2f5738a5",
                  "ActionId": 5000365
                }
              ]
            }
          ]
        },
        {
          "Id": 12,
          "Speaker": "Speaker_83",
          "PlotLineKey": "狮尾骤然扫过石柱，你翻滚着躲开飞溅的碎石，左肩传来布料绽裂的凉意。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1585,
              "PlotLineKey": "主动进攻。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4111
                  },
                  "ActionGuid": "d0c861c5b506406c9ed634db292a9c44",
                  "ActionId": 5000366
                }
              ]
            },
            {
              "TextId": 1586,
              "PlotLineKey": "防御。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4112
                  },
                  "ActionGuid": "ca2dd8d6c818433591823585b3b41fd8",
                  "ActionId": 5000367
                }
              ]
            }
          ]
        },
        {
          "Id": 13,
          "Speaker": "Speaker_83",
          "PlotLineKey": "观众席爆发的惊呼被狮吼截断，<br>声浪裹挟着沙粒擦过耳畔，<br>你尝到唇齿间的血腥味。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1587,
              "PlotLineKey": "迅速处理伤口。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4121
                  },
                  "ActionGuid": "575981a9f81548ca917b5e8850e945e3",
                  "ActionId": 5000368
                }
              ]
            },
            {
              "TextId": 1588,
              "PlotLineKey": "忍耐，伤口以后再说。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4122
                  },
                  "ActionGuid": "057b8ec7daf540caa6f04fad94fa191a",
                  "ActionId": 5000369
                }
              ]
            }
          ]
        },
        {
          "Id": 14,
          "Speaker": "Speaker_83",
          "PlotLineKey": "第二波攻击来得毫无预兆，狮像前掌拍地的瞬间，你来不及反应，只能用手中的剑正面迎击。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1589,
              "PlotLineKey": "回想起之前战斗的历练。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4131
                  },
                  "ActionGuid": "eb57cc3fc5674f269d0aebf9328cd1ed",
                  "ActionId": 5000370
                }
              ]
            },
            {
              "TextId": 1590,
              "PlotLineKey": "这一剑，将献出我的所有。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4132
                  },
                  "ActionGuid": "e74e73f3d8ab40eabb8cc235c23c324e",
                  "ActionId": 5000371
                }
              ]
            }
          ]
        },
        {
          "Id": 17,
          "Speaker": "Speaker_83",
          "PlotLineKey": "你抵挡住了亚狮诺索的凶猛进攻，在它撤力的一瞬，找准机会将剑刃刺入它右前肢的古老裂痕。<br>灿金的血液浸染了竞技台。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1591,
              "PlotLineKey": "稳住自己的呼吸。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4141
                  },
                  "ActionGuid": "05a4947f9bdb41548c22a8c978c76799",
                  "ActionId": 5000372
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700037
                  },
                  "ActionGuid": "c36cbd33e6a14b5fa062bd9136e83299",
                  "ActionId": 5000423
                },
                {
                  "Name": "ReignsChangePlayerName",
                  "Params": {
                    "PlayerNameId": "ReignsPlayerName_5"
                  },
                  "ActionGuid": "6c3a73d8aae642208bfe75155f66fb6a",
                  "ActionId": 5000424
                }
              ]
            },
            {
              "TextId": 1592,
              "PlotLineKey": "抬头望向亚狮诺索。",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4142
                  },
                  "ActionGuid": "5490d235bf964d4ab888ad1e08497abb",
                  "ActionId": 5000373
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700037
                  },
                  "ActionGuid": "cf0b1d5ec8a548199c3dbdf1b6ffe2a0",
                  "ActionId": 5000425
                },
                {
                  "Name": "ReignsChangePlayerName",
                  "Params": {
                    "PlayerNameId": "ReignsPlayerName_5"
                  },
                  "ActionGuid": "97e557fe7e0f43a4af09725a12c5fd59",
                  "ActionId": 5000426
                }
              ]
            }
          ]
        },
        {
          "Id": 18,
          "Speaker": "Speaker_83",
          "PlotLineKey": "亚狮诺索不再攻击，它的鬃毛拂过你淌血的面颊，随后俯下身躯。<br>霎时，看台上爆发出雷鸣般的掌声，<br>人们在高呼你的名字，以呼嚎向你致意。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1593,
              "PlotLineKey": "我通过了最后的试炼。",

      {
        "Id": 7,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Hör gut zu. Es war eine regnerische Nacht – genau wie heute.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 8,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Eine Studentin von der Abteilung für Leerenmaterie hatte gerade ein Forschungsprojekt abgeschlossen und wartete mit aufgespanntem Regenschirm an einer Übertragungsstation.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 9,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Die Übertragungsstation, sonst immer geschäftig, war bis auf sie menschenleer. Der Wind heulte, der Regen strömte und die Nacht vertiefte sich.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 10,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Ehe sie sich versah, gingen auch die Lichter aus, und der Transport war nirgends zu sehen.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 11,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Zitternd hielt sie ihren Regenschirm umklammert – es war das Einzige, woran sie sich festhalten konnte.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 12,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Endlich hielt ein Pfadfinder neben ihr. Der Innenraum war pechschwarz, und sie stieg panisch an Bord.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 13,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Sie beruhigte sich erst, nachdem der Fahrer ihr zulächelte und ihr zunickte.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 14,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "Als sie jedoch am Fenster saß und den Blick über die unheimliche Übertragungsstation schweifen ließ, bemerkte sie plötzlich …",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 15,
        "Speaker": " I.R.I.S.",
        "PlotLineKey": "… Wie eine Gestalt mit Regenschirm dort stand, die genau wie sie aussah.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2026_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },

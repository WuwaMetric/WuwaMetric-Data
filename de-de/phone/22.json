      },
      {
        "Id": 6,
        "Speaker": " Yangyang",
        "PlotLineKey": "Herzlichen Glückwunsch zum Geburtstag, {PlayerName}.",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_1_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 7,
        "Speaker": " Yangyang",
        "PlotLineKey": "Ich hab mehrere Nachrichten verfasst, aber egal, wie viel ich geschrieben hab, irgendwie reichten Worte einfach nicht aus. Dann hab ich meine Nachricht mehrmals gelöscht und überarbeitet …",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_1_UI.png",
        "RolePileIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRolePile/T_IconRole_Pile_anke_UI.png"
      },
      {
        "Id": 8,
        "Speaker": " Yangyang",
        "PlotLineKey": "Bevor ich mich entschieden hab, es einfach zu halten, wie es sich für einen Anlass wie deinen Geburtstag gehört.",
        "PlayVoice": false,
        "Options": [
          {
            "TextId": 112,
            "PlotLineKey": "Vielen Dank für deine Glückwünsche.",
            "PlayVoice": false,
            "JumpToId": 9
          },
          {
            "TextId": 113,
            "PlotLineKey": "Ich verstehe, was du meinst.",
            "PlayVoice": false,
            "JumpToId": 10
          },
          {
            "TextId": 114,
            "PlotLineKey": "Ich werde mir heute was Gutes gönnen.",
            "PlayVoice": false,
            "JumpToId": 11
          }
        ],

{
  "Id": 700002,
  "Name": "Epos der glühenden Sonne – Kapitel II",
  "Description": "Kampftraining",
  "Type": "Reigns",
  "StateKeyPattern": "剧情_2_5_七丘主线_下半间章_7_1",
  "Chapters": [
    {
      "Title": "Epos der glühenden Sonne – Kapitel II",
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "Nestor",
          "PlotLineKey": "Die Morgendämmerung bricht mit einem blassen Schimmer an. Ein Monat des Trainings mit Cato ist bereits vergangen.<br>Und heute geht die harte Arbeit weiter.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1124,
              "PlotLineKey": "An den Trainingsplan halten.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2001
                  },
                  "ActionGuid": "cbe84a6ae16b4f1fb0474a5242a7f433",
                  "ActionId": 5000043
                }
              ]
            },
            {
              "TextId": 1125,
              "PlotLineKey": "Eine kleine Pause kann nicht schaden.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2002
                  },
                  "ActionGuid": "ec14b1d28bd74fd6a0619ff779e5b627",
                  "ActionId": 5000159
                }
              ]
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Du gehst zum Training.<br>Das viele Wiederholen der Bewegungsmuster macht dir langsam zu schaffen.<br>Plötzlich siehst du ein paar Gladiatoren, die dir unbekannte Bewegungen üben. Du wirst …",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1126,
              "PlotLineKey": "Dich konzentrieren und weiterüben.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2011
                  },
                  "ActionGuid": "a4559aefe70b499f8f030cbfd43c3b4b",
                  "ActionId": 5000160
                }
              ]
            },
            {
              "TextId": 1127,
              "PlotLineKey": "Sie beobachten und ihre Bewegungsmuster lernen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2012
                  },
                  "ActionGuid": "da5ff3e8ff414a8e8008a327017df98b",
                  "ActionId": 5000161
                }
              ]
            }
          ]
        },
        {
          "Id": 3,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Du denkst über dein Training nach. Du warst stets alleine mit deinem Holzpfahl und musstest dich immer wieder selbst aufraffen.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1128,
              "PlotLineKey": "Durchhalten und weitermachen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2021
                  },
                  "ActionGuid": "86f6038611004d678b0f969ac37430bc",
                  "ActionId": 5000162
                }
              ]
            },
            {
              "TextId": 1129,
              "PlotLineKey": "Dich der Traurigkeit hingeben.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2022
                  },
                  "ActionGuid": "738c2da6da9246368611eec00e469771",
                  "ActionId": 5000163
                }
              ]
            }
          ]
        },
        {
          "Id": 4,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Die Tage vergehen und der Agon der Gladiatoren rückt näher.<br>Heute bringt dir Cato eine letzte Sache bei …",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1130,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2031
                  },
                  "ActionGuid": "3d779ec6d06b45db9a0e348e3f61988d",
                  "ActionId": 5000164
                }
              ]
            },
            {
              "TextId": 1131,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2032
                  },
                  "ActionGuid": "d5fd8e9d43bd47daa27aca4c0d624c82",
                  "ActionId": 5000165
                }
              ]
            }
          ]
        },
        {
          "Id": 5,
          "Speaker": "Cato",
          "PlotLineKey": "Ich habe alles getan, um dich vorzubereiten. Jetzt bist du auf dich allein gestellt.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1132,
              "PlotLineKey": "Endet das Training heute?",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2041
                  },
                  "ActionGuid": "abd82d2f356d49dab390b7a474e2e0b1",
                  "ActionId": 5000166
                }
              ]
            },
            {
              "TextId": 1133,
              "PlotLineKey": "Ist es meine Abschlussfeier?",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2042
                  },
                  "ActionGuid": "c98357ebe3b54d9f8bf761bec5580acf",
                  "ActionId": 5000167
                }
              ]
            }
          ]
        },
        {
          "Id": 6,
          "Speaker": "Cato",
          "PlotLineKey": "Hier ist meine letzte Aufgabe für dich: Sag mir, warum willst du Gladiatorin werden? <br>Was motiviert dich beim Training?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1134,
              "PlotLineKey": "Du kennst die Antwort bereits.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2051
                  },
                  "ActionGuid": "cac9927cc490430f8d2a4e424f7fc9a0",
                  "ActionId": 5000168
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700045
                  },
                  "ActionGuid": "352380b28b0843af9b96e14ee7e65037",
                  "ActionId": 5000413
                }
              ]
            },
            {
              "TextId": 1135,
              "PlotLineKey": "Du bist dir nicht sicher.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2052
                  },
                  "ActionGuid": "85360aeb373b4e0d9ddabb5c1385b2df",
                  "ActionId": 5000169
                },
                {
                  "Name": "ReignsChangeBackground",
                  "Params": {
                    "Background": "/Game/Aki/UI/UIResources/Common/Image/BgCgBig/T_ReignsBg3.T_ReignsBg3",
                    "ChangeMode": 1
                  },
                  "ActionGuid": "c6283045cb2c4d7a9ab03619d8d48d15",
                  "ActionId": 5000434
                }
              ]
            }
          ]
        },
        {
          "Id": 9,
          "Speaker": "Nestor",
          "PlotLineKey": "Seit du dein Zuhause verlassen hast, hast du nie an deinem Ziel gezweifelt: stark genug zu sein, um andere zu beschützen.<br>Dein Blick trifft Catos und er liest dir die Antwort von den Augen ab.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1136,
              "PlotLineKey": "Ich halte an meiner Ambition fest.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2061
                  },
                  "ActionGuid": "f9649b854dcc42889afcf3afe9f61b90",
                  "ActionId": 5000170
                }
              ]
            },
            {
              "TextId": 1137,
              "PlotLineKey": "Cato danken.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2062
                  },
                  "ActionGuid": "a2de1aa833dd4fd1af018364584b9980",
                  "ActionId": 5000171
                }
              ]
            }
          ]
        },
        {
          "Id": 10,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Ein Funken Begierde flackert in deinen Augen auf.<br>Du erinnerst dich an Catos Worte, als all dies begann:",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1138,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2071
                  },
                  "ActionGuid": "3ba77b305a534a639495219c223aad80",
                  "ActionId": 5000172
                }
              ]
            },
            {
              "TextId": 1139,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2072
                  },
                  "ActionGuid": "c983a033b98d45888cee0670e2bb9422",
                  "ActionId": 5000173
                }
              ]
            }
          ]
        },
        {
          "Id": 11,
          "Speaker": "Cato",
          "PlotLineKey": "„Jeder wird dann deinen Namen rufen: Augusta.<br>Du wirst ihre Herzen erobern und ihren Applaus gewinnen müssen, selbst ohne Identität, Status und Talent.<br>Sag mir, werde ich diesen Tag erleben?“",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1140,
              "PlotLineKey": "Ich kann es schaffen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2081
                  },
                  "ActionGuid": "5aaaae4be153490885ec4c178ccc6294",
                  "ActionId": 5000174
                }
              ]
            },
            {
              "TextId": 1141,
              "PlotLineKey": "Ich freue mich schon darauf.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2082
                  },
                  "ActionGuid": "626cc5e7434f42a6a2eb637459d3bc6e",
                  "ActionId": 5000175
                }
              ]
            }
          ]
        },
        {
          "Id": 12,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Du weißt, dass die Sponsoren des Agon der Gladiatoren meistens für dich unerreichbare Adlige und reiche Kaufleute sind, außer vielleicht in der Untergrundarena …",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1142,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2091
                  },
                  "ActionGuid": "0aa5eb033ab24aae86b314ab68adafd5",
                  "ActionId": 5000176
                },
                {
                  "Name": "PostAkEvent",
                  "Params": {
                    "EventConfig": {
                      "Type": "Global",
                      "AkEvent": "/Game/Aki/WwiseAudio/Events/setstate_music_state_2_5_kingship_chapter_01.setstate_music_state_2_5_kingship_chapter_01"
                    }
                  },
                  "ActionGuid": "5b30b35f78214695a58ba75555d3798d",
                  "ActionId": 5000391
                },
                {
                  "Name": "ReignsChangeBackground",
                  "Params": {
                    "Background": "/Game/Aki/UI/UIResources/Common/Image/BgCgBig/T_ReignsBg9.T_ReignsBg9",
                    "ChangeMode": 1
                  },
                  "ActionGuid": "4225f2a8d439421e97ddcf5df0a3d9b1",
                  "ActionId": 5000397
                }
              ]
            },
            {
              "TextId": 1143,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2092
                  },
                  "ActionGuid": "e193bbc0002d42bf8ec13d36f93a4ce0",
                  "ActionId": 5000177
                },
                {
                  "Name": "PostAkEvent",
                  "Params": {
                    "EventConfig": {
                      "Type": "Global",
                      "AkEvent": "/Game/Aki/WwiseAudio/Events/setstate_music_state_2_5_kingship_chapter_01.setstate_music_state_2_5_kingship_chapter_01"
                    }
                  },
                  "ActionGuid": "c42051017b484203aa8bc6a4eaeaf515",
                  "ActionId": 5000398
                },
                {
                  "Name": "ReignsChangeBackground",
                  "Params": {
                    "Background": "/Game/Aki/UI/UIResources/Common/Image/BgCgBig/T_ReignsBg9.T_ReignsBg9",
                    "ChangeMode": 1
                  },
                  "ActionGuid": "4aa5cc4975a649758093d84485da2e49",
                  "ActionId": 5000399
                }
              ]
            }
          ]
        },
        {
          "Id": 13,
          "Speaker": "Untergrundhändler",
          "PlotLineKey": "Hehe. Das hier ist alles erstklassige „Ware“, die ich hier habe. Komm mal vorbei und schau dich um!",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1144,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2101
                  },
                  "ActionGuid": "05317d825fcc44c8b1f78dd5415cd2bb",
                  "ActionId": 5000178
                }
              ]
            },
            {
              "TextId": 1145,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2102
                  },
                  "ActionGuid": "58b667d7f41a4fa693ce8df06df24c4c",
                  "ActionId": 5000179
                }
              ]
            }
          ]
        },
        {
          "Id": 14,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Wie Marktware stehst du mit einigen anderen Gladiatoren mitten in der Untergrundarena und wirst von Adligen und reichen Kaufleuten auf den Tribünen begutachtet. <br>Einer nach dem anderen werden die anderen neben dir ausgewählt und weggeführt.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1146,
              "PlotLineKey": "Einen Schritt nach vorne treten.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2111
                  },
                  "ActionGuid": "9527897eaef44483aaea0406a2082617",
                  "ActionId": 5000180
                }
              ]
            },
            {
              "TextId": 1147,
              "PlotLineKey": "Dich anbieten.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2112
                  },
                  "ActionGuid": "54806687e4ee46c0900b07d7768c5971",
                  "ActionId": 5000181
                }
              ]
            }
          ]
        },
        {
          "Id": 15,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Die Blicke der Zuschauer richten sich auf dich.<br>Als du dich vorstellst, brechen sie in Gelächter aus.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1148,
              "PlotLineKey": "Die Visionen vom Ruin deines Zuhauses flimmern in deinem Geist.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2121
                  },
                  "ActionGuid": "aee347b279064a089752d02141219340",
                  "ActionId": 5000182
                }
              ]
            },
            {
              "TextId": 1149,
              "PlotLineKey": "In den verschwenderischen Lebensstil der Adligen huschen und vorbeiblicken.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2122
                  },
                  "ActionGuid": "b4f5f006a0c543e5a2ef0596556a0bb2",
                  "ActionId": 5000183
                }
              ]
            }
          ]
        },
        {
          "Id": 16,
          "Speaker": "Augusta",
          "PlotLineKey": "Dieses Lachen … Kann es sein, dass sie tatsächlich Teil dieses Unheils waren?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1150,
              "PlotLineKey": "Es war nicht nur die Dunkle Flut, die dein Heim zerstörte.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2131
                  },
                  "ActionGuid": "ac6e04150313450ca1a5441db232ac11",
                  "ActionId": 5000184
                }
              ]
            },
            {
              "TextId": 1151,
              "PlotLineKey": "Der Verfall in den hohen Kreisen trug ebenfalls zum Untergang deines Zuhauses bei.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2132
                  },
                  "ActionGuid": "17982c40291a4404bbcd4cd75a802e60",
                  "ActionId": 5000185
                }
              ]
            }
          ]
        },
        {
          "Id": 17,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Als die Dunkle Flut kam, lebten die Mächtigen und Reichen weiter in Saus und Braus.<br>Die starken Krieger von Septimont existierten nur, um ihren Gelüsten zu dienen.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1152,
              "PlotLineKey": "Ein Feuer der Wut entflammt in deiner Brust.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2141
                  },
                  "ActionGuid": "032d03ec9d424154839c132fca7b82ca",
                  "ActionId": 5000186
                }
              ]
            },
            {
              "TextId": 1153,
              "PlotLineKey": "Stillschweigend schwörst du dir, diese Regeln zu brechen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2142
                  },
                  "ActionGuid": "3d350a8524164144a8b0af212862aef3",
                  "ActionId": 5000187
                }
              ]
            }
          ]
        },
        {
          "Id": 18,
          "Speaker": "Arroganter Adliger",
          "PlotLineKey": "Na so was, ein Mädchen vom Land von unscheinbarer Stärke, das von Macht und Kraft träumt.<br>Ich weiß nicht, wann Septimont das letzte Mal so einen lustigen Witz gehört hat.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1154,
              "PlotLineKey": "Ich brauche nur eine Chance, mich zu beweisen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2151
                  },
                  "ActionGuid": "e8e61a843b494ea582a579d104f87096",
                  "ActionId": 5000188
                }
              ]
            },
            {
              "TextId": 1155,
              "PlotLineKey": "Das Schwert zeigen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2152
                  },
                  "ActionGuid": "3131aa2bbbe745f6bf4f4a34e5afb8bb",
                  "ActionId": 5000189
                }
              ]
            }
          ]
        },
        {
          "Id": 19,
          "Speaker": "Arroganter Adliger",
          "PlotLineKey": "Was hast du zu bieten?<br>Dich wie ein Zirkusclown aufführen und um dein Leben rennen, damit wir alle etwas zum Lachen haben?<br>Oder glaubst du, dass du als Unschuldslamm punkten kannst?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1156,
              "PlotLineKey": "Es ist nicht meine Art, andere zu enttäuschen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2161
                  },
                  "ActionGuid": "16b0373ca09e4bc9b6106e4298c65a95",
                  "ActionId": 5000190
                }
              ]
            },
            {
              "TextId": 1157,
              "PlotLineKey": "Unterschätze mich nicht.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2162
                  },
                  "ActionGuid": "07e9b772e831417d82e7f79bed130645",
                  "ActionId": 5000191
                }
              ]
            }
          ]
        },
        {
          "Id": 20,
          "Speaker": "Arroganter Adliger",
          "PlotLineKey": "Ist jemand interessiert?<br>Es ist schließlich immer unterhaltsam, den Schwachen beim Kämpfen zuzusehen.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1158,
              "PlotLineKey": "Du wirfst den Adligen einen eiskalten Blick zu, während sie leise flüstern.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2171
                  },
                  "ActionGuid": "4749d8518f3b4d8ba1df230390d8fba7",
                  "ActionId": 5000192
                }
              ]
            },
            {
              "TextId": 1159,
              "PlotLineKey": "Ein kleines Gefühl der Erwartung durchflutet dich, dass dich jemand auswählen wird.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 2172
                  },
                  "ActionGuid": "55fa0bea4cbc40d2b4ea83c172196e4c",
                  "ActionId": 5000193
                }
              ]
            }
          ]
        },
        {
          "Id": 21,
          "Speaker": "Altmodischer Adliger",

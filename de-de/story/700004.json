{
  "Id": 700004,
  "Name": "Epos der glühenden Sonne – Kapitel IV",
  "Description": "Tag der Krönung",
  "Type": "Reigns",
  "StateKeyPattern": "剧情_2_5_七丘主线_下半间章_9_1",
  "Chapters": [
    {
      "Title": "Epos der glühenden Sonne – Kapitel IV",
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "Nestor",
          "PlotLineKey": "Am Vorabend der Prüfung kehrst du noch einmal auf das Trainingsgelände zurück.<br>Während du auf Cato wartest, stehst du vor dem vertrauten Holzpfahl.<br>Er war dein ständiger Begleiter bei deinem Streben nach wahrer Stärke.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1563,
              "PlotLineKey": "Ihn sanft umarmen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4001
                  },
                  "ActionGuid": "e9b93f612e404f3793ccbace2e242af1",
                  "ActionId": 5000334
                }
              ]
            },
            {
              "TextId": 1564,
              "PlotLineKey": "Ihn abtrennen und die Vergangenheit hinter dir lassen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4002
                  },
                  "ActionGuid": "f3cc6b8ce75e4b0091095dfdc1fb89d1",
                  "ActionId": 5000335
                }
              ]
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "Cato",
          "PlotLineKey": "Nun? Kein besonders einfacher Weg, nicht wahr?<br>Verrate mir, was hast du auf diesem Weg gelernt?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1565,
              "PlotLineKey": "Stärke kann Furcht einflößen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4011
                  },
                  "ActionGuid": "ffb72e8c99934e1eac65f25357ed780a",
                  "ActionId": 5000336
                }
              ]
            },
            {
              "TextId": 1566,
              "PlotLineKey": "Stärke kann zerstörerisch sein.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4012
                  },
                  "ActionGuid": "36cc12119718458192873219ec8e2369",
                  "ActionId": 5000337
                }
              ]
            }
          ]
        },
        {
          "Id": 3,
          "Speaker": "Cato",
          "PlotLineKey": "Stärke bringt Eroberung. So lautet die älteste Regel der Menschheit.<br>Augusta, ich werde bei deiner letzten Prüfung dabei sein und so laut deinen Namen rufen, dass ihn jeder hört.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1567,
              "PlotLineKey": "Ich werde hier nicht aufgeben.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4021
                  },
                  "ActionGuid": "099e06b7c82f4a128dbb207cc023bae7",
                  "ActionId": 5000338
                }
              ]
            },
            {
              "TextId": 1568,
              "PlotLineKey": "Danke.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4022
                  },
                  "ActionGuid": "a99854e1f3dd409984c2ba90076d4ed3",
                  "ActionId": 5000339
                }
              ]
            }
          ]
        },
        {
          "Id": 4,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Zurück in deinem Zimmer starrst du ausdruckslos auf deine Sammlung von Schwertern.<br>Sie symbolisieren Stärke und Eroberung, aber wie wirst du dies der Welt beweisen?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1569,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4031
                  },
                  "ActionGuid": "8cfd81094b564b0092cf78da4752790e",
                  "ActionId": 5000340
                }
              ]
            },
            {
              "TextId": 1570,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4032
                  },
                  "ActionGuid": "12dd603a359b4e199766ebb2b907f514",
                  "ActionId": 5000341
                }
              ]
            }
          ]
        },
        {
          "Id": 5,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Einst hörtest du die Geschichte von einem legendären Gladiator.<br>Ein Krieger, der nach einem Sieg stets die Schwerter des Gegners mit seinem eigenen verschmolz.<br>Nun beschließt du, denselben Weg zu beschreiten und stehst vor der Schmiede.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1571,
              "PlotLineKey": "Vor der Schmiede innehalten.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4041
                  },
                  "ActionGuid": "7ca9eca50c284a23a6f1f128f117f123",
                  "ActionId": 5000342
                }
              ]
            },
            {
              "TextId": 1572,
              "PlotLineKey": "Die fliegenden Funken der Schmiede betrachten.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4042
                  },
                  "ActionGuid": "c57fbba4a4e64f50bc7a444f9fed0027",
                  "ActionId": 5000343
                }
              ]
            }
          ]
        },
        {
          "Id": 6,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Du willst deine Stärke demonstrieren, doch nun versuchst du immer noch, dies mit Macht zu erreichen, die nicht deine eigene ist …<br>Beim Beobachten des Schmiedehammers, der sich hebt und senkt, entsteht eine scheinbar unpraktische Idee.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1573,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4051
                  },
                  "ActionGuid": "8f3b0787d4944a1691f74986ccd7ea23",
                  "ActionId": 5000344
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700050
                  },
                  "ActionGuid": "f1db0b9da2b54f88bcc6650fd31d9161",
                  "ActionId": 5000421
                }
              ]
            },
            {
              "TextId": 1574,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4052
                  },
                  "ActionGuid": "3060ae7df3f74cb48f27ceafc28564ef",
                  "ActionId": 5000345
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700050
                  },
                  "ActionGuid": "7bd76620331648e0be3a66aebf373ce4",
                  "ActionId": 5000422
                }
              ]
            }
          ]
        },
        {
          "Id": 7,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Schließlich setzt du deine Resonanzkraft ein, um alle Schwerter zu einem einzigen zu verschmelzen.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1575,
              "PlotLineKey": "Das schwere Schwert in deiner Hand umschließen.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4061
                  },
                  "ActionGuid": "a50233bbc9c543a4910e9035817c9eed",
                  "ActionId": 5000346
                },
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 49
                  },
                  "ActionGuid": "fa8dc593f9b6405994d077a8842be0b3",
                  "ActionId": 5000348
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700023
                  },
                  "ActionGuid": "61ffe3aeead24aa18eab039e8377eca4",
                  "ActionId": 5000355
                }
              ]
            },
            {
              "TextId": 1576,
              "PlotLineKey": "Die Macht spüren.",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4062
                  },
                  "ActionGuid": "68b8e076182447c9957d266256d7547d",
                  "ActionId": 5000347
                },
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 49
                  },
                  "ActionGuid": "4f106cbdd1cb4f99bd145c0511f6d2c3",
                  "ActionId": 5000356
                },
                {
                  "Name": "ReignsCallCard",
                  "Params": {
                    "CardId": 700023
                  },
                  "ActionGuid": "cf1949875ec446cba6b4f2a76e876c11",
                  "ActionId": 5000357
                }
              ]
            }
          ]
        },
        {
          "Id": 8,
          "Speaker": "Speaker_83",
          "PlotLineKey": "Deine Kraft fällt unter den Resonatoren nicht auf.<br>Doch diese scheinbar unbedeutende Kraft genügt, um stärker zu sein als das Schmiedefeuer und Hammerschwünge.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 1577,
              "PlotLineKey": "(Fortfahren)",
              "PlayVoice": false,
              "JumpToId": null,
              "Actions": [
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4071
                  },
                  "ActionGuid": "24e70f98e9324f08a9522bac5afa69f0",
                  "ActionId": 5000358
                },
                {
                  "Name": "ReignsAddBuff",
                  "Params": {
                    "BuffId": 4072
                  },
                  "ActionGuid": "64456d09b7584ca79ca85191a8029046",
                  "ActionId": 5000406
                },
                {
                  "Name": "PostAkEvent",
                  "Params": {
                    "EventConfig": {
                      "Type": "Global",
                      "AkEvent": "/Game/Aki/WwiseAudio/Events/setstate_music_state_2_5_kingship_chapter_02.setstate_music_state_2_5_kingship_chapter_02"

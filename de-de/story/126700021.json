  "Chapters": [
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Hey, {PlayerName}? Was hat dich hierher gebracht?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 81,
              "PlotLineKey": "Einfach nur herumwandern.",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Du schaffst es immer, auf wichtige Orte zu stoßen.",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Das hier ist ein heiliger Dating-Ort für Paare. Es wird gesagt, dass, wenn Paare zu diesem Teich kommen und einen Wunsch zum Himmel schicken, während die Frösche quaken, sie ein Leben lang zusammenbleiben können.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 82,
              "PlotLineKey": "Ergibt für mich keinen Sinn.",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 4,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Diese grasbewachsene Böschung und der Teich sind tatsächlich nichts Besonderes. Der Schlüssel liegt jedoch im Ruf der Schwarzgestreiften Frösche.",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "Schwarzgestreifter Frosch",
          "PlotLineKey": "Alwa, alwa, alwa.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 83,
              "PlotLineKey": "...",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 6,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Klingt das nicht wie \"immer\"? Paare denken, das bedeutet \"immer und ewig\", und wünschen, dass dies ihre Liebe segnet.",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 7,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Sie geben den Schwarzgestreiften Fröschen sogar einen Spitznamen, \"Alwa Frosch\".",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 8,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Nun, es sind nur Paare, die ihre eigenen Erwartungen auf ein Tier projizieren.",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 84,
              "PlotLineKey": "Es ist eine kulturelle Sache.",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 9,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Nun, für mich ist es eher eine Glaube-kann-Berge-versetzen Sache.",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 10,
          "Speaker": "Xuanyin",
          "PlotLineKey": "Wie auch immer, wir schweifen ein wenig ab. Lassen wir das Modul zuerst installieren.",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 11,
          "Speaker": "Speaker_undefined",
          "PlotLineKey": "Xuanyin hat das Froschsprache-Übersetzungsmodul auf dein Pangu-Terminal geladen.",
          "PlayVoice": false,
          "Options": []
        },

  "Chapters": [
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "เฮ้ {PlayerName}มาทำอะไรที่นี่เหรอ",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 81,
              "PlotLineKey": "แค่เดินดูรอบ ๆ น่ะ",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "คุณไปถึงสถานที่สำคัญตลอดเลยนะ",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "นี่คือที่นัดพบศักดิ์สิทธิ์สำหรับคู่รัก ว่ากันว่าถ้าทั้งคู่มาที่สระนี้และอธิษฐานต่อฟ้าขณะที่กบร้อง ทั้งสองคนจะได้อยู่เคียงข้างกันไปชั่วชีวิต",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 82,
              "PlotLineKey": "ฟังดูไม่ค่อยเข้าใจเท่าไหร่",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 4,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "จริงอยู่ว่าเนินหญ้าลาดชันและสระน้ำนี้ไม่พิเศษอะไร แต่สิ่งสำคัญคือเสียงร้องของกบคาดดำ",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "กบลายดำ",
          "PlotLineKey": "อ๊บอะ อ๊บอะ อ๊บอะ",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 83,
              "PlotLineKey": "...",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 6,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "ฟังดูคล้าย \"อมตะ\" ไหม พวกคู่รักคิดว่ามันหมายถึง \"ตลอดไปและตลอดไป\" เป็นพรที่จะทำให้รักของพวกเขาเป็นนิรันดร์",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 7,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "พวกเขาเลยตั้งชื่อเล่นให้กบคาดดำว่า \"กบอ๊บอะ\"",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 8,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "คู่รักก็ตีความพวกสัตว์ตามที่ตัวเองมอง",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 84,
              "PlotLineKey": "เป็นวัฒนธรรมอย่างหนึ่ง",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 9,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "สำหรับฉันแล้ว นี่เป็นเรื่องความเชื่อนำไปสู่สิ่งเหลือเชื่อ",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 10,
          "Speaker": "เสวียนยิน",
          "PlotLineKey": "พวกเราชักจะออกทะเลกันไปไกลแล้ว มาติดตั้งโมดูลกันก่อนเถอะ",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 11,
          "Speaker": "Speaker_undefined",
          "PlotLineKey": "สวนยินโหลดโมดูลแปลภาษากบเข้าในเทอร์มินัลผานกู่ของคุณ",
          "PlayVoice": false,
          "Options": []
        },

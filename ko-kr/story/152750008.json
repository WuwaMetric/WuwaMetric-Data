  "Chapters": [
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "Speaker_83",
          "PlotLineKey": "가벼우면서 몽환적인 주파수가 북받쳐 오른다",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "???",
          "PlotLineKey": "보여요... 저 별이...",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "???",
          "PlotLineKey": "당신은 여전히 굳건히 뚜렷하게 빛나고 있군요",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "???",
          "PlotLineKey": "전 여기서 제 사명을 완수할 거예요",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "???",
          "PlotLineKey": "하지만 이때 당신은 어디에 있을까요?",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 6,
          "Speaker": "???",
          "PlotLineKey": "황룡은 당신이 바라던 데로 됐는지... 모르겠네요",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "Speaker_83",
          "PlotLineKey": "차가우면서 이성을 강조하는 주파수가 북받쳐 오른다",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "???",
          "PlotLineKey": "... 권한 확인 완료. 뭇별의 조율자",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "???",
          "PlotLineKey": "이제부터 본 세대 솔라리스 기반의 코드 베이스를 구축하기 시작합니다",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "???",
          "PlotLineKey": "예상 소요 시간은 763시간 21분입니다",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "???",
          "PlotLineKey": "코드 목표: 비명으로 인해 특수화된 주파수",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "바로 눈앞에 있어요! 마지막 단서만 남았어요!",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "곧 승리예요!",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "어때요. 띵하고 보물이 나타나지 않았나요!",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 46,
              "PlotLineKey": "그러진 않았어요...",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "그럴 리가요! 그럼 팩 게임 속의 내용이랑은 다른데...",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "어떤 것들이 있었나요?",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 47,
              "PlotLineKey": "소리만 있던데요?",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 4,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "알겠어요... 휴. 하지만 그렇다고 헛수고할 수는 없죠",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "그래도 클리어 보상은 얻어야죠! 보물 관련된 건 제가 좀 더 알아볼게요",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 6,
          "Speaker": "쿠 · 카스 (통신 중)",
          "PlotLineKey": "오시면서 찍으신 사진은 제가 사진첩으로 만들어서 준비해 둘게요!",
          "PlayVoice": false,
          "Options": []
        }
      ]
    }
  ]

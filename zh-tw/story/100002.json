    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "白芷",
          "PlotLineKey": "無法複現嗎？",
          "PlayVoice": true,
          "Options": [
            {
              "TextId": 417,
              "PlotLineKey": "（搖頭。）",
              "PlayVoice": false,
              "JumpToId": null
            },
            {
              "TextId": 418,
              "PlotLineKey": "沒有感覺。",
              "PlayVoice": false,
              "JumpToId": null
            },
            {
              "TextId": 419,
              "PlotLineKey": "沒有之前被牽引的感受。",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "熾霞",
          "PlotLineKey": "難道只有厲害的殘象漂泊者才能肉身吸收嗎？",
          "PlayVoice": true,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "熾霞",
          "PlotLineKey": "或者需要是特定種類？",
          "PlayVoice": true,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "熾霞",
          "PlotLineKey": "還是……",
          "PlayVoice": true,
          "Options": []
        },
        {
          "Type": "Outline",
          "Id": 8,
          "Speaker": "Narrator",
          "PlotLineKey": "你再次嘗試肉身吸收聲骸卻失敗了，眾人也一時間搞不清其中的規律。",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "熾霞",
          "PlotLineKey": "要不然用終端試試？",
          "PlayVoice": true,
          "Options": []
        }
      ]
    },
    {

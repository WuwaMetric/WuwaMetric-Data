        "Speaker": "KU-Ragi",
        "PlotLineKey": "¡{PlayerName}! ¡El Observatorio Intergaláctico vuelve a estar en línea! ¡Es absolutamente increíble!",
        "PlayVoice": false,
        "Options": [],
        "HeadIconAsset": "https://api-v2.encore.moe/resource/Data/Game/Aki/UI/UIResources/Common/Image/IconRoleHeadCircle256/T_IconRoleHeadCircle256_2006_UI.png",

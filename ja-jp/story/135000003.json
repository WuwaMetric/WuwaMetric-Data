  "Chapters": [
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "？？？",
          "PlotLineKey": "おかしいな……どこ行ったんだろう……",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 45,
              "PlotLineKey": "何か探してる？",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "？？？",
          "PlotLineKey": "私の猫……るるを探してるの。走り回るのが好きで、私にべったりなコなんだ。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "？？？",
          "PlotLineKey": "私これから今州城に仕入れに行かなきゃいけないんだけど……るるの事がどうしても心配で……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "橘",
          "PlotLineKey": "ああ、ごめん！自我紹介がまだだったね。私は橘、この飲食店の女将だよ。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "橘",
          "PlotLineKey": "食事に来てくれたのかな？でもごめんね、今は営業時間外なんだ。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 46,
              "PlotLineKey": "猫探し、手伝うよ。",
              "PlayVoice": false,
              "JumpToId": 6
            },
            {
              "TextId": 47,
              "PlotLineKey": "ついさっき、るるを見たけど……",
              "PlayVoice": false,
              "JumpToId": 7
            }
          ]
        },
        {
          "Id": 6,
          "Speaker": "橘",
          "PlotLineKey": "ホント！？ありがとう！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 7,
          "Speaker": "橘",
          "PlotLineKey": "ホント！？どこにいた！？",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 48,
              "PlotLineKey": "{Male=俺が連れ戻してくるよ。;Female=私が連れ戻してくるよ。}",
              "PlayVoice": false,
              "JumpToId": 8
            }
          ]
        },
        {
          "Id": 8,
          "Speaker": "橘",
          "PlotLineKey": "ありがとう！",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "橘",
          "PlotLineKey": "わっ、早かったね！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "橘",
          "PlotLineKey": "ああ……るる……戻って来てくれてよかった……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "橘",
          "PlotLineKey": "ほらママの話を聞いて？ね？今度またこんなことしたら、るるを縄で縛って、ぶら下げちゃうよ？",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "橘",
          "PlotLineKey": "そしたら……嬉しい時はるるにいっぱいキスして、怒った時はるるを力いっぱいに揉みつぶしちゃう！どう？どう？どう？",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "橘",
          "PlotLineKey": "こう、んんんー！ああ、もう一回！んんんー！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 6,
          "Speaker": "橘",
          "PlotLineKey": "ふふ、ふふふ……えへへっ、えへ……",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 59,
              "PlotLineKey": "！！！",
              "PlayVoice": false,
              "JumpToId": null
            },
            {
              "TextId": 60,
              "PlotLineKey": "……",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 7,
          "Speaker": "橘",
          "PlotLineKey": "おっと危ない。人がいるんだった……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 8,
          "Speaker": "橘",
          "PlotLineKey": "あまり気にしないで？私はただ……ちょっと猫が好きすぎるだけで……",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 61,
              "PlotLineKey": "わかる……！",
              "PlayVoice": false,
              "JumpToId": 9
            },
            {
              "TextId": 62,
              "PlotLineKey": "わからなくもないけど……",
              "PlayVoice": false,
              "JumpToId": 10
            }
          ]
        },
        {
          "Id": 9,
          "Speaker": "橘",
          "PlotLineKey": "ホント！？ドン引きされたらどうしようって思ったけど……えへへっ……そうだ、あなたもるると遊んであげて！キスしても大丈夫だよ。このコ、結構そういうの好きなんだ！",
          "PlayVoice": false,
          "Options": [],
          "JumpToId": 10
        },
        {
          "Id": 10,
          "Speaker": "橘",
          "PlotLineKey": "ともあれ、るるを探してくれてありがとう！そうだ、このコの世話はあなたに任せてもいい？",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 63,
              "PlotLineKey": "{Male=望むところだ！;Female=やったあ！}",
              "PlayVoice": false,
              "JumpToId": null
            },
            {
              "TextId": 64,
              "PlotLineKey": "けっ、結構です！",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        }
      ]
    },

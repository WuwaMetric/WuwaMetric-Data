    },
    {
      "Dialogues": [
        {
          "Id": 2,
          "Speaker": "ハゲ猫",
          "PlotLineKey": "ウニャーウー",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "喜んでますね。これなら翻訳しなくてもわかります！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "ハゲ猫（翻訳）",
          "PlotLineKey": "しもべ！うまかったのじゃ！大儀であったぞ！*文字化け*",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "わ、またモジュールにエラーが発生したようですね……。ひとまず外しましょうか……むしろよくここまで正常に動作しましたね……。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 6,
          "Speaker": "Speaker_undefined",
          "PlotLineKey": "【懸音はあなたのデバイスから猫語翻訳モジュールを外し、データを記録した】",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 7,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "言語ロジックにまだ改善の余地が……やはり、私では正式な研究員には及びませんね……。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 90,
              "PlotLineKey": "なんとなく言ってることはわかった。",
              "PlayVoice": false,
              "JumpToId": 8
            },
            {
              "TextId": 91,
              "PlotLineKey": "頑張って、応援してる。",
              "PlayVoice": false,
              "JumpToId": 9
            }
          ]
        },
        {
          "Id": 8,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "そうですね……出来はそんなに悪くないと思いますし。",
          "PlayVoice": false,
          "Options": [],
          "JumpToId": 10
        },
        {
          "Id": 9,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "失敗の繰り返しは研究に必要な過程ですもんね。",
          "PlayVoice": false,
          "Options": [],
          "JumpToId": 10
        },
        {
          "Id": 10,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "おかげさまでデータを集めることができました。帰ったら分析に使います。今回はありがとうございました。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 11,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "次会うまでに……また新しい動物言語翻訳モジュールを開発して驚かせちゃいます！えへへ！",
          "PlayVoice": false,
          "Options": []
        }
      ]
    }

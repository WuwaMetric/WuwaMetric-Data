    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "どうしてこんなことに……",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 60,
              "PlotLineKey": "完全に、事切れたね……",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "はあ……これも、真理に近づくための尊い犠牲です。 群彩たちの名前は、論文の謝辞に入れましょう。",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 61,
              "PlotLineKey": "{Male=魚の隣に俺の名前が並ぶのはちょっと……;Female=魚の隣に私の名前が並ぶのはちょっと……}",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 3,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "そう言わないであげてください。 協力してくれた全ての動物への感謝の気持ちは本物です！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 4,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "今回は、尊い犠牲ということで、それはあなたにあげます。群彩の肉はなかなか美味しいと聞きましたよ……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 5,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "この翻訳モジュールは、今回故障が多かったから、念入りに確認しないと……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 6,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "文字化けしてないなって思っていたら、 起動して早々に落ちてしまったていたようです。それでは外しますね。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 8,
          "Speaker": "懸音（ケンオン）",
          "PlotLineKey": "ではまたお会いしましょう、{PlayerName}。",
          "PlayVoice": false,
          "Options": []
        }
      ]
    }

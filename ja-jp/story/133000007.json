      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "仕事に取り込む研究員",
          "PlotLineKey": "まだデータ分析中で……",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "書類点検している研究員",
          "PlotLineKey": "はあ……まだ終わってないのかよ……",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "1番演者",
          "PlotLineKey": "またオーダーが変更された……これで何度目だ……",
          "PlayVoice": false,
          "Options": []
        },
        {

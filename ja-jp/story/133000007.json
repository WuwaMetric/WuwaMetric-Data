      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "初一",
          "PlotLineKey": "このデータカードを上にいる研究員に届けてもらえますか？",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 133,
              "PlotLineKey": "いいよ。",
              "PlayVoice": false,
              "JumpToId": 2
            },
            {
              "TextId": 134,
              "PlotLineKey": "自分でやれば？",
              "PlayVoice": false,
              "JumpToId": 3
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "初一",
          "PlotLineKey": "本当ですか？ありがとうございます！",
          "PlayVoice": false,
          "Options": [],
          "JumpToId": 4
        },
        {
          "Id": 3,
          "Speaker": "初一",
          "PlotLineKey": "勤務中なので、ここを離れられないんです……",
          "PlayVoice": false,
          "Options": [],
          "JumpToId": 4
        },
        {
          "Id": 4,
          "Speaker": "初一",
          "PlotLineKey": "もちろん、これは僕の私用ですので、予め報酬は払います！お願いします！",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "どこから来たのよ、このいたずら猿たち——！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 2,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "——っと危ない。やつらを怒らせたら余計大変なことになる。",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "……まったく、どこから来たよ！いくらでもお礼してあげるから、このいたずら猿を駆逐できる人いないかな。",
          "PlayVoice": false,
          "Options": []
        }
      ]
    },
    {
      "Dialogues": [
        {
          "Id": 1,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "どこから来たよ、このいたずら猿の群れ……",
          "PlayVoice": false,
          "Options": [
            {
              "TextId": 164,
              "PlotLineKey": "全部追い出したよ。",
              "PlayVoice": false,
              "JumpToId": null
            }
          ]
        },
        {
          "Id": 2,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "ほ、本当？すごい！ありがとう！これは報酬だよ！",
          "PlayVoice": false,
          "Options": []
        },
        {
          "Id": 3,
          "Speaker": "焦っている研究員",
          "PlotLineKey": "まさか、こんなにたくさんいたずら猿が来るなんて……",
          "PlayVoice": false,
          "Options": []
        }
      ]
